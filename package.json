{
  "name": "micror",
  "version": "1.0.0",
  "description": "A micro client router based in pagejs",
  "main": "micror.js",
  "scripts": {
    "codestyle": "xo",
    "test": "npm run codestyle && browserify test/*.js | testron | tap-spec"
  },
  "xo": {
    "env": {
      "browser": true,
      "node": true
    },
    "space": true,
    "rules": {
      "consistent-return": 0,
      "no-void": 0,
      "no-nested-ternary": 0,
      "no-inline-comments": 0,
      "object-curly-spacing": 0,
      "space-before-function-paren": 0,
      "no-undef": 0,
      "brace-style": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expalmer/micror.git"
  },
  "keywords": [],
  "author": "Palmer Oliveira <expalmer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^12.0.1",
    "electron-prebuilt": "^0.36.3",
    "hihat": "^2.6.0",
    "tap-dev-tool": "^1.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "testron": "^1.2.0",
    "xo": "^0.12.1"
  }
}
